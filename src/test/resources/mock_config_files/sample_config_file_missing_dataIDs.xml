<loafr>
    <events>
        <event name="System_Initialization">
            <data_element name="Status"/>
            <data_element name="Error_Code"/>
        </event>
        <event name="Communication_Check">
            <data_element name="Result"/>
            <data_element name="Error_Code"/>
            <data_element name="Frequencies"/>
            <data_element name="Signal_Strength"/>
        </event>
        <event name="Navigation_Test">
            <data_element name="Result"/>
            <data_element name="Error_Code"/>
            <data_element name="Accuracy"/>
        </event>
        <event name="Telemetry_Check">
            <data_element name="Result"/>
            <data_element name="Error_Code"/>
            <data_element name="Metrics"/>
        </event>
        <event name="Battery_Test">
            <data_element name="Result"/>
            <data_element name="Error_Code"/>
            <data_element name="Battery_Level"/>
            <data_element name="Charging"/>
            <data_element name="Remaining_Capacity"/>
            <data_element name="Voltage"/>
        </event>
        <event name="Redundancy_Check">
            <data_element name="Result"/>
            <data_element name="Error_Code"/>
            <data_element name="Redundancy_Levels"/>
            <data_element name="Redundancy_Percentage"/>
        </event>
        <event name="Thruster_Calibration">
            <data_element name="Status"/>
            <data_element name="Error_Code"/>
            <data_element name="Power_Levels"/>
            <data_element name="Fuel_Remaining"/>
        </event>
        <event name="Pre-Launch_Check">
            <data_element name="Status"/>
            <data_element name="Error_Code"/>
            <data_element name="Authorization"/>
            <data_element name="Result"/>
            <data_element name="Probability_of_Success"/>
        </event>
        <event name="Engine_Ignition">
            <data_element name="Status"/>
            <data_element name="Error_Code"/>
            <data_element name="Fuel_Type"/>
            <data_element name="Fuel_Remaining"/>
            <data_element name="Thrust_Power"/>
        </event>
        <event name="Orbital_Maneuver">
            <data_element name="Status"/>
            <data_element name="Thrust_Power"/>
            <data_element name="Angles"/>
            <data_element name="Metrics"/>
        </event>
        <event name="Solar_Panel_Deployment">
            <data_element name="Status"/>
            <data_element name="Panel_Status"/>
            <data_element name="Error_Code"/>
            <data_element name="Temperature"/>
            <data_element name="Angles"/>
        </event>
    </events>
    <data_elements>

    </data_elements>
